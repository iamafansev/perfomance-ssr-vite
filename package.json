{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/server/server.dev.ts",
    "serve": "NODE_ENV=production node dist/server/server.prod.js",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/server/server.prod.ts",
    "lint": "eslint './**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.8.1",
    "serve-static": "^1.15.0",
    "vite": "^4.1.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.7",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "eslint": "7.32.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}
